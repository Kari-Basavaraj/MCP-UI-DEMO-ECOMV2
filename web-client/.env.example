# OpenRouter API key (get one at https://openrouter.ai/keys)
OPENROUTER_API_KEY=sk-or-v1-your-key-here

# Agentation (enabled by default in development unless set to false)
NEXT_PUBLIC_AGENTATION_ENABLED=true

# Optional Agentation sync endpoint and webhook forwarding URL
# If NEXT_PUBLIC_AGENTATION_ENDPOINT is not set, dev default is http://localhost:4747
# If NEXT_PUBLIC_AGENTATION_WEBHOOK_URL is not set, dev default is http://localhost:8787/api/agentation/webhook
# NEXT_PUBLIC_AGENTATION_ENDPOINT=http://localhost:4747
# NEXT_PUBLIC_AGENTATION_WEBHOOK_URL=http://localhost:8787/api/agentation/webhook

# ── Figma Webhook Receiver (production) ──────────────────────
